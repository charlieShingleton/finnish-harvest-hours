{
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-console": 1,
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "env": {
    "es6": true,
    "node": true
  }
}
